<application>
<name>Joint Attention System Demo</name>
<dependencies>
      <port>/root</port>
      <port>/icub/camcalib/left</port>
      <port>/icub/camcalib/right</port>
      <port>/icub/head/state:o</port>
</dependencies>
   <module>
      <name>salience</name>
      <parameters>--filter color --name /salleft</parameters>
	  <node>johnny5</node>
	<tag>salleft</tag>
   </module>
   <module>
      <name>salience</name>
      <parameters>--filter color --name /salright</parameters>
	  <node>johnny5</node>
	<tag>salright</tag>
   </module>
   <module>
      <name>egoRemapper</name>
      <parameters>--from /home/niehaus4/uiuc-lar/vision/stereoAttention/conf/bertwarp.ini</parameters>
      <node>johnny5</node>
	<tag>egomap</tag>
   </module>
   <module>
      <name>stereoAttention</name>
      <parameters>--from /home/niehaus4/uiuc-lar/vision/stereoAttention/conf/bertwarp.ini --voffset -20</parameters>
	  <node>johnny5</node>
	<tag>stereoattn</tag>
   </module>
   <module>
      <name>stereoIOR</name>
      <parameters>--from /home/niehaus4/uiuc-lar/vision/stereoAttention/conf/iorbert.ini</parameters>
      <node>johnny5</node>
	<tag>stereoior</tag>
   </module>
<connection>
  <from>/icub/head/state:o</from>
  <to>/egoRemapper/pos:h</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/head/state:o</from>
  <to>/stereoIOR/pos:h</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/stereoAttention/fxl:o</from>
  <to>/stereoIOR/ior:i</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/egoRemapper/agg:l</from>
  <to>/stereoAttention/ego:l</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/egoRemapper/agg:r</from>
  <to>/stereoAttention/ego:r</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/egoRemapper/sal:lo</from>
  <to>/stereoAttention/sal:l</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/egoRemapper/sal:ro</from>
  <to>/stereoAttention/sal:r</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/stereoIOR/sal:lo</from>
  <to>/egoRemapper/map1:l</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/stereoIOR/sal:ro</from>
  <to>/egoRemapper/map1:r</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/egoRemapper/sal:lo</from>
  <to>/stereoIOR/sal:l</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/egoRemapper/sal:ro</from>
  <to>/stereoIOR/sal:r</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/salleft/map</from>
  <to>/egoRemapper/map0:l</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/salright/map</from>
  <to>/egoRemapper/map0:r</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/camcalib/left/out</from>
  <to>/salleft/view</to>
  <protocol>tcp</protocol>
</connection>
<connection>
  <from>/icub/camcalib/right/out</from>
  <to>/salright/view</to>
  <protocol>tcp</protocol>
</connection>
</application>

